<h1>Postagens registradas</h1> <hr>
{{#each posts}}

<h2>{{dataValues.titulo}}</h2>
<p>{{dataValues.conteudo}}</p>
<small>{{dataValues.createdAt}}</small>
<a href="/deletar/{{dataValues.id}}"><button>Deletar Post</button></a>
<a href="/editar/{{dataValues.id}}"><button>Editar Post</button></a>
<hr>
{{/each}}

<a href="/cad"><button>Adicionar Novo Post</button></a> 
